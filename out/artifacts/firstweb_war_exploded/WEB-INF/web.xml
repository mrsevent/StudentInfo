<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>registerServlet</servlet-name>
        <servlet-class>com.servlet.registerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>registerServlet</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>queryStudentInfoServlet</servlet-name>
        <servlet-class>com.servlet.queryStudentInfoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>queryStudentInfoServlet</servlet-name>
        <url-pattern>/queryStudentInfo</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CheckCode</servlet-name>
        <servlet-class>com.code.CheckCode</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckCode</servlet-name>
        <url-pattern>/CheckCode</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>loginServlet</servlet-name>
        <servlet-class>com.servlet.loginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>editStudentInfoServlet</servlet-name>
        <servlet-class>com.servlet.editStudentInfoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>editStudentInfoServlet</servlet-name>
        <url-pattern>/editStudentInfoServlet</url-pattern>
    </servlet-mapping>
</web-app>